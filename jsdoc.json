{
  "plugins": ["node_modules/better-docs/category"],
  "recurseDepth": 10,
  "source": {
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": ""
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": ["category", "component"],
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "better-docs": {
      "navLinks": [
        {
          "label": "Github",
          "href": "https://github.com/coreyjwhite/phetch"
        }
      ]
    }
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true,
    "template": "node_modules/better-docs",
    "readme": "./README.md"
  }
}
